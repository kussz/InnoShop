@model int;
@{
    ViewData["Title"] = "Создание населённого пункта";
}

<div>
    <hr />
        <dl class="row">
            <dd class="col-sm-10" id="nameContainer">
                <form id="nameForm" method="post">
                    <div class="form-group">
                        <label for="name" class="control-label">Населённый пункт:</label>
                        <input type="text" id="name" name="name" class="form-control" value="" required />
                    </div>
                </form>
            </dd>
        </dl>
    <div id="errors" class="text-danger"></div>

        <div>
            <a asp-action="Index" id="formButton" class="btn btn-primary">Создать</a> |
            <a asp-action="Index" class="btn btn-close">Вернуться</a>
        </div>
</div>
@section Scripts
{
    <script src="~/js/Post.js" asp-append-version="true"></script>
}
@section Prescripts
{
    <script>
        tableName = "Locality";
        methods = "Create";
    </script>
}
