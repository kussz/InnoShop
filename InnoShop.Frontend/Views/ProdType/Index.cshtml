@model IEnumerable<InnoShop.Domain.Models.ProdType>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Категории";
}

<h2>Категории</h2>

<div id="loading" style="display: block;">Загрузка...</div>
<div id="loadedItems" style="display: none;">
<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Название
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody id="itemsContainer">
        
    </tbody>
</table>
</div>
@section Prescripts
{
    <script src="~/js/AdminRequired.js" asp-append-version="true"></script>
}
@section Scripts
{
    <script src="~/js/GetEnumerable.js" asp-append-version="true"></script>
    <script>
        async function func() { 
        await fetchItems("http://localhost:5036/ProdType",'ProdType');
        }
        func();
    </script>
}